{
    "manifest_version": 3,
    "name": "Zoom Toggler",
    "version": "0.0.7",
    "description": "Quickly toggle browser page zoom between two levels",
    "permissions": [
        "storage"
    ],
    "options_page": "options.html",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}
